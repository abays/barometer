LoadPlugin procevent

<Plugin "procevent">
  BufferLength {{ procevent_buffer_length }}
{% for process in procevent_processes %}
  Process {{ process }}
{% endfor %}
{% for regex_process in procevent_regex_processes %}
  RegexProcess {{ regex_process }}
{% endfor %}
</Plugin>
